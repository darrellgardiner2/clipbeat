# Convex - run `npx convex dev` to configure
EXPO_PUBLIC_CONVEX_URL=

# Clerk - get from https://dashboard.clerk.com
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Pipeline - set at least one download + recognition
# Download: yt-dlp service (primary) or Apify (fallback)
YT_DLP_SERVICE_URL=
YT_DLP_API_KEY=
APIFY_API_TOKEN=

# Audio extraction from video (when Apify returns video only)
CONVERTAPI_SECRET=

# Vocal removal (Replicate Demucs)
REPLICATE_API_TOKEN=

# Music recognition (ACRCloud fallback - ShazamKit is on-device)
ACRCLOUD_HOST=identify-us-west-2.acrcloud.com
ACRCLOUD_ACCESS_KEY=
ACRCLOUD_ACCESS_SECRET=
